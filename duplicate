 #include,stdio.h>
 int main()
 {
 int size,i,a[10],n,i,j,k;
 printf("enter n");
 scanf("%d",&n);
 printf("enter elements of an array");
 for(i-0;i<n;i++)
 scanf("d",&a[i]);
 for(i=0;i<n;i++)
 {
 for(j=i+1;j<n;j++)
 {
 if(a[i]==a[j])
 {
 for(k=j;k<size;k++)
 {
 a[k]=a[k+1];
 }
 n-- ;
 j--;
 }
 }
 }
 printf("the final array is ");
 printf("%d",a[i]);
 }
 }
  